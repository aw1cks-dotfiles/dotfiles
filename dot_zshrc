zstyle ':completion:*:default' list-colors ${(s.:.)LS_COLORS}
export FZF_DEFAULT_OPTS='--height 30% --layout=reverse'
export STEAM_COMPAT_DATA_PATH=$HOME/.proton

chpwd() {
    printf "\033]%s\07\n" "zsh"
}

mp3towav() {
    ffmpeg -i "$1.mp3" -acodec pcm_u8 -ar 22050 "$1.wav"
}

clip() {
    cat $1 | xclip -sel clip
}

paste() {
  if [ $# -eq 0 ] ; then
    cat /dev/stdin | tee /dev/tty | curl -X PUT -d @- https://p.mort.coffee
  else
    echo "Uploading: $1"
    curl --upload-file $1 https://p.mort.coffee
  fi
}

mkp() {
  mkdir -p $1
  pushd $1
}

lvpnup() {
  (cd $HOME/Documents/git/openconnect; sh run.sh)
}

lvpndown() {
  (cd $HOME/Documents/git/openconnect; sh stop.sh)
}

alias grep='/bin/grep --color=auto'
alias l='ls -alh'
alias ll='ls --color -l'
alias ls='ls --color=auto'
alias vi='nvim'
alias vim='nvim'
alias sudo='sudo '
alias ip='ip -c'
alias ga='git add'
alias gc='git clone'
alias gcom='git commit -m'
alias gpu='git pull'
alias gp='git push'
alias grv="git remote -v"
alias gcm='git checkout master'
alias tmux='tmux -u'
alias antibody-update="antibody bundle < ~/.config/zsh_plugins.txt > ~/.config/zsh_plugins.sh"
alias yayskip='yay -S --mflags --skipinteg'
alias ytm='youtube-dl -x --audio-format mp3'
alias lvpnlog='docker logs -f ldnvpn'

[ -f "~/.config/zsh_plugins.sh" ] && source ~/.config/zsh_plugins.sh || antibody-update
source ~/.config/gbt
source /usr/share/fzf/key-bindings.zsh

